{
  "name": "node-esbuild-template",
  "version": "0.0.1",
  "private": true,
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizle-kit migrate",
    "db:push": "drizzle-kit push",
    "build": "rm -rf ./dist && node esbuild.js",
    "start": "node ./dist/index.js",
    "dev": "nodemon --config nodemon.json",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "prettify": "prettier . -w"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "drizzle-orm": "^0.36.3",
    "hono": "^4.6.10",
    "postgres": "^3.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.28.1",
    "esbuild": "^0.18.17",
    "nodemon": "^3.0.1",
    "npm-dts": "^1.3.12",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  }
}
